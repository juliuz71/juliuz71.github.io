---
layout: none
permalink: /search.json
---

[
    {% for post in site.posts %}
        {% if post.layout == 'post' %}
            {
                "title"     : "{{ post.title }}",
                "desc"      : "{{ post.title }}",
                "category"  : "{{ post.category }}",
                "url"       : "{{ post.url }}",
                "item"      : "{{ post.item }}",
                "volume"    : "{{ post.volume }}",
                "image"     : "{{ post.image }}"
            } {% if forloop.last %}{% else %},{% endif %}
        {% endif %}
    {% endfor %}
]